<?xml version="1.0" encoding="UTF-8" ?>
<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing
        http://symfony.com/schema/routing/routing-1.0.xsd">
    <!-- only load in dev environment! -->
    <route id="_emsch_templates" path="/templates">
        <default key="_controller">emsch.controller.twig_list::list</default>
    </route>
    <route id="_emsch_template" path="/template">
        <default key="_controller">emsch.controller.twig_list::template</default>
    </route>
</routes>